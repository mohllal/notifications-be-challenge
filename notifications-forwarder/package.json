{
  "name": "notifications-forwarder",
  "version": "1.0.0",
  "description": "Notifications forwarder service",
  "main": "main.js",
  "scripts": {
    "transpile": "babel ./ --out-dir dist --ignore node_modules --copy-files",
    "start": "npm run dev",
    "serve": "node ./dist/main",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./main",
    "prod": "cross-env NODE_ENV=production node ./dist/main",
    "compose": "cross-env NODE_ENV=compose node ./dist/main",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf dist",
    "lint": "eslint -c .eslintrc.json --ext .js .",
    "lint:fix": "eslint -c .eslintrc.json --ext .js . --fix",
    "test": "cross-env NODE_ENV=testing mocha './test/**/*.test.js' --exit --recursive --require @babel/register"
  },
  "author": "Kareem Khaled",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bullmq": "^1.36.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "firebase-admin": "^9.10.0",
    "hbs": "^4.1.2",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.14",
    "nanoid": "^3.1.23",
    "npm-run-all": "^4.1.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.14.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "mocha": "^9.0.1",
    "supertest": "^6.1.3"
  }
}
